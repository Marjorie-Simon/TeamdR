<%= simple_form_for(@event) do |f| %>
  <%= f.input :start_time, as: :string, required: false, input_html: {class: "datepicker"}, label: :Start %>
  <%= f.input :end_time, as: :string, required: false, input_html: {class: "datepicker"}, label: :End %>
  <%= f.association :invitee, collection: @berlin_office, label_method: :full_name  %>
  <%= f.input :title, label: :Subject %>
  <%= f.input :content, label: :Message %>
<%= f.submit %>
<% end %>
